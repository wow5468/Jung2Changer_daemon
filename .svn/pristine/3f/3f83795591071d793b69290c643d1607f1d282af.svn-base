package com.nexters.jung2.daemon.sentence;

import java.util.HashMap;

public class Jung2Conf
{
  private String strSentence;
  private HashMap<String, String> mapAvailableWords;

  public Jung2Conf(String strSentence, HashMap<String, String> mapAvailableWords)
  {
    this.strSentence = strSentence;
    this.mapAvailableWords = mapAvailableWords;
  }

  public String getStrSentence() {
    return this.strSentence;
  }

  public HashMap<String, String> getMapAvailableWords() {
    return this.mapAvailableWords;
  }
}