package com.nexters.jung2.daemon.sentence;

import java.util.ArrayList;
import java.util.HashMap;

public class SentenceConf
{
  private String strSentence;
  private HashMap<String, ArrayList<String>> mapMP;

  public SentenceConf(String strSentence, HashMap<String, ArrayList<String>> mapMP)
  {
    this.strSentence = strSentence;
    this.mapMP = mapMP;
  }

  public String getStrSentence() {
    return this.strSentence;
  }

  public HashMap<String, ArrayList<String>> getMapMP() {
    return this.mapMP;
  }
}